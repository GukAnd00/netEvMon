{
  "name": "netevmon",
  "version": "1.0.0",
  "description": "Network monitoring system with ElasticSearch",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "dev": "npm run build && npm start",
    "build": "npm run clean && npm run build-server",
    "clean": "rimraf ./dist && ./node_modules/.bin/mkdirp ./dist",
    "validate": "eslint ./src/",
    "validate-fix": "eslint --fix ./src/ && npm run validate",
    "build-server": "babel src -d ./dist --copy-files --source-maps"
  },
  "author": "guk.and00",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@babel/polyfill": "^7.12.1",
    "logops": "^2.1.1",
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "email-templates": "^7.1.2",
    "googleapis": "^71.0.0",
    "express-fileupload": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "express-logging": "^1.1.1",
    "express-request-id": "^1.4.1",
    "body-parser": "^1.19.0",
    "ip": "^1.1.5",
    "cors": "^2.8.5",
    "mongoose": "^5.10.11",
    "nodemailer": "^6.4.15",
    "@elastic/elasticsearch": "^7.12.0",
    "lodash": "^4.17.21",
    "useragent": "^2.3.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-source-map-support": "^2.1.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "http-server": "^0.12.3"
  }
}
